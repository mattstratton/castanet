{
  "name": "castanet",
  "version": "1.0.2",
  "description": "A podcast-oriented theme for Hugo",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "hugo -s exampleSite",
    "css:dev": "tailwindcss -i ./assets/css/main.css -o assets/css/index.css --watch",
    "css:build": "tailwindcss -i ./assets/css/main.css -o assets/css/index.css --minify",
    "dev-hugo": "hugo server -s exampleSite --gc --themesDir=../..",
    "dev-tailwind-old": "npx @tailwindcss/cli -i assets/css/main.css -o assets/css/index.css --watch",
    "dev": "run-p dev-hugo css:dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mattstratton/castanet.git"
  },
  "author": "Matt Stratton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattstratton/castanet/issues"
  },
  "homepage": "https://github.com/mattstratton/castanet#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "jquery": "^3.5.1",
    "jssocials": "^1.5.0",
    "mediaelement": "^4.2.14",
    "mediaelement-plugins": "^2.5.0",
    "netlify-cli": "^17.21.1"
  }
}
