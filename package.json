{
  "name": "castanet",
  "version": "2.0.0",
  "description": "A podcast-oriented theme for Hugo",
  "main": "index.js",
  "scripts": {
    "preinstall": "rimraf assets/lib && rimraf static/webfonts && rimraf static/img/lib",
    "postinstall": "vendor-copy",
    "assets": "rimraf assets/lib  && rimraf static/webfonts && rimraf static/img/lib && vendor-copy",
    "css:dev": "NODE_ENV=development npx @tailwindcss/cli  -i ./assets/css/main.css -o ./assets/css/compiled/main.css --jit -w",
    "css:build": "NODE_ENV=production npx @tailwindcss/cli  -i ./assets/css/main.css -o ./assets/css/compiled/main.css --jit",
    "build": "hugo -s exampleSite",
    "example": "hugo server --bind 0.0.0.0 -p 8008 --source exampleSite --themesDir ../.. --buildDrafts -b http://localhost/castanet/",
    "dev-hugo": "hugo server -s exampleSite --gc --themesDir=../..",
    "dev": "run-p css:dev dev-hugo",
    "lighthouse": "lhci autorun"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mattstratton/castanet.git"
  },
  "keywords": [
    "hugo",
    "hugo-theme",
    "tailwind",
    "dark-mode"
  ],
  "author": "Matt Stratton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattstratton/castanet/issues"
  },
  "homepage": "https://github.com/mattstratton/castanet#readme",
  "devDependencies": {
    "@lhci/cli": "^0.14.0",
    "@tailwindcss/cli": "^4.1.6",
    "autoprefixer": "^10.4.17",
    "netlify-cli": "^21.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1",
    "vendor-copy": "^3.0.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "jquery": "^3.5.1",
    "jssocials": "^1.5.0",
    "mediaelement": "^4.2.14",
    "mediaelement-plugins": "^2.5.0"
  },
  "vendorCopy": [],
  "devVendorCopy": [
    {
      "from": "node_modules/jquery/dist/jquery.js",
      "to": "assets/lib/jquery/jquery.js"
    },
    {
      "from": "node_modules/jssocials/dist/jssocials.js",
      "to": "assets/lib/jssocials/jssocials.js"
    },
    {
      "from": "node_modules/jssocials/dist/jssocials.css",
      "to": "assets/lib/jssocials/css/jssocials.css"
    },
    {
      "from": "node_modules/jssocials/dist/jssocials-theme-plain.css",
      "to": "assets/lib/jssocials/css/jssocials-theme-plain.css"
    },
    {
      "from": "node_modules/mediaelement/build/mediaelement-and-player.js",
      "to": "assets/lib/mediaelement/mediaelement-and-player.js"
    },
    {
      "from": "node_modules/mediaelement/build/mediaelementplayer.min.css",
      "to": "assets/lib/mediaelement/css/mediaelementplayer.min.css"
    },
    {
      "from": "node_modules/mediaelement/build/mejs-controls.svg",
      "to": "static/img/lib/mediaelement/mejs-controls.svg"
    },
    {
      "from": "node_modules/mediaelement-plugins/dist/speed/speed.js",
      "to": "assets/lib/mediaelement/speed.js"
    },
    {
      "from": "node_modules/mediaelement-plugins/dist/speed/speed.min.css",
      "to": "assets/lib/mediaelement/css/speed.min.css"
    },
    {
      "from": "node_modules/mediaelement-plugins/dist/speed/speed-i18n.js",
      "to": "assets/lib/mediaelement/speed-i18n.js"
    },
    {
      "from": "node_modules/@fortawesome/fontawesome-free/css/all.css",
      "to": "assets/lib/fontawesome/css/all.css"
    },
    {
      "from": "node_modules/@fortawesome/fontawesome-free/webfonts",
      "to": "static/webfonts"
    }
  ]
}
